{
  "compilerOptions": {
    "types": ["@2gis/gl-matrix"],
    "target": "ES2020",
    "module": "ES6",
    "moduleResolution": "node",
    "lib": ["ES2020", "DOM"],
    "sourceMap": true,
    "strict": true,
    "noEmitHelpers": true,
    "importHelpers": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true
  }
}
